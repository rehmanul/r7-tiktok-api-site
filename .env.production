# Production Environment Configuration

# EnsembleData API
ENSEMBLEDATA_TOKEN=your_ensembledata_token_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Database (for future features)
# Use a secure connection string. Do not store secrets in plaintext in the repo.
DATABASE_URL=postgresql://postgres:your_db_password@postgres:5432/tiktok_api

# API Configuration
ENVIRONMENT=production
API_VERSION=2.0.0

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_REQUESTS_PER_HOUR=5000

# Cache
CACHE_TTL=300

# Monitoring (optional)
SENTRY_DSN=
PROMETHEUS_ENABLED=true
