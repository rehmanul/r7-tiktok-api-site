# Optional TikTok cookie string or JSON array
TIKTOK_COOKIE=

# Individual cookie overrides (used if TIKTOK_COOKIE is empty)
TIKTOK_SESSION_ID=
TIKTOK_WEBID=

# Rate limiting (set to 0 to disable a window)
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_REQUESTS_PER_HOUR=1000

# Response caching
CACHE_TTL=120
CACHE_MAX_ENTRIES=100

# Puppeteer timeouts (milliseconds)
NAVIGATION_TIMEOUT_MS=30000
CONTENT_WAIT_MS=5000

# Local server port
PORT=3000
