{
    "buildCommand": "echo 'Build complete'",
    "outputDirectory": "public",
    "functions": {
        "api/tiktok.js": {
            "memory": 2048,
            "maxDuration": 60
        }
    },
    "build": {
        "env": {
            "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
        }
    },
    "env": {
        "CHROME_PATH": "/tmp/chromium"
    }
}